    "@prisma/client": "^7.2.0",
    "@prisma/extension-accelerate": "^1.2.1",
    "@socket.io/redis-adapter": "^8.3.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "livekit-server-sdk": "^2.15.0",
    "multer": "^2.0.2",
    "multer-s3": "^3.0.1",
    "pdf-parse": "^2.4.5",
    "pg": "^8.16.3",
    "redis": "^5.10.0",
    "resend": "^6.6.0",
    "socket.io": "^4.8.3",
    
    
    "isomorphic-dompurify": "^2.16.0",
    "validator": "^13.15.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.6",
