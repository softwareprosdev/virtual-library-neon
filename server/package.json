{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "npx ts-node src/__tests__/manual-test-api.ts",
    "test:api:prod": "API_URL=https://api.indexbin.com/api npx ts-node src/__tests__/manual-test-api.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "sh -c \"echo 'RUN_ADMIN_SETUP='\\\"$RUN_ADMIN_SETUP\\\"''; npx prisma migrate deploy || echo 'Migration failed, continuing...'; if [ -n \\\"$RUN_ADMIN_SETUP\\\" ] && echo \\\"$RUN_ADMIN_SETUP\\\" | grep -Eqi '^(1|true|yes|y)$'; then echo 'Running admin setup...'; node src/scripts/setupAdmin.js || echo 'Admin setup failed (continuing)'; else echo 'Skipping admin setup.'; fi; node dist/index.js\"",
    "db:hard-reset": "npx prisma migrate reset --force",
    "db:export": "ts-node scripts/export-database.ts",
    "dev": "ts-node src/index.ts",
    "setup:admin": "node src/scripts/setupAdmin.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
